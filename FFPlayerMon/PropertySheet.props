<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <SDL2LibDir>$(ProjectDir)..\SDL2\</SDL2LibDir>
    <FFMpegLibDir>$(ProjectDir)..\FFmpeg\</FFMpegLibDir>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="SDL2LibDir">
      <Value>$(SDL2LibDir)</Value>
    </BuildMacro>
    <BuildMacro Include="FFMpegLibDir">
      <Value>$(FFMpegLibDir)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>